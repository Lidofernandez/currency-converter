{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": ["jsx-a11y"],
  "rules": {
    "max-len": ["error", { "code": 80, "ignoreUrls": true }],
    "no-console": [2],
    "require-await": [2]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
